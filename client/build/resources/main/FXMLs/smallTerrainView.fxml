<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="60.0" prefWidth="60.0" type="AnchorPane"
         xmlns="http://javafx.com/javafx/11.0.1">
    <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="100.0"
          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0">
        <Label fx:id="terrainNameLabel" alignment="BOTTOM_CENTER" maxHeight="1.7976931348623157E308"
               maxWidth="1.7976931348623157E308" style="-fx-background-color: #ffffff;" text="Name" VBox.vgrow="ALWAYS">
            <padding>
                <Insets bottom="10.0"/>
            </padding>
        </Label>
        <Label fx:id="terrainValueLabel" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308"
               maxWidth="1.7976931348623157E308" style="-fx-background-color: #ffffff;" text="Value"
               VBox.vgrow="ALWAYS">
            <padding>
                <Insets top="10.0"/>
            </padding>
        </Label>
    </VBox>
</fx:root>
